name: on_push
on:
  push:

jobs:
  on_push:
    runs-on: ubuntu-latest
    steps:
    - name: Check no quotes
      run: echo "no quotes ${{ github.event.repository.default_branch }}"
      if: github.ref_name == ${{ github.event.repository.default_branch }}

    - name: Check quotes
      run: echo "quotes ${{ github.event.repository.default_branch }}"
      if: github.ref_name == '${{ github.event.repository.default_branch }}'
